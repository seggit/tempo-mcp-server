# Tempo MCP Server Environment Configuration
# Copy this file to .env and fill in your actual values

# Required: Your Tempo API Token
# Get this from: Tempo → Settings → API Integration
TEMPO_API_TOKEN=your_tempo_api_token_here

# Optional: Tempo API Base URL (defaults to v4.0)
TEMPO_BASE_URL=https://api.tempo.io/4

# Optional: Enable debug logging
TEMPO_DEBUG=false

# Optional: Custom rate limiting (requests per second)
# Default is 5 requests per second to respect Tempo's limits
# TEMPO_RATE_LIMIT=5

# Optional: HTTP timeout in seconds
# TEMPO_TIMEOUT=30

# Optional: Your Jira base URL for enhanced error messages
# JIRA_BASE_URL=https://your-company.atlassian.net
